    "#BOX BLUR\n",
    "#Import required Image library\n",
    "from PIL import Image, ImageFilter\n",
    "\n",
    "#Open existing image\n",
    "OriImage = Image.open('nature.jpg')\n",
    "OriImage.show()\n",
    "\n",
    "blurImage = OriImage.filter(ImageFilter.BoxBlur(4))\n",
    "blurImage.show()\n"
   ]
